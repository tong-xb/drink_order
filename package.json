{
  "name": "order-system",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:debug": "vite build --mode debugpro",
    "preview": "vite preview --host",
    "test:unit:dev": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ci": "vitest run",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173/order-system/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview :4173/order-system/ 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .eslintignore",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|html|jsonc|vue)\"",
    "svgo": "npx svgo --recursive src/assets"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "animate.css": "^4.1.1",
    "async-validator": "^4.2.5",
    "axios": "^1.2.6",
    "crypto-js": "^4.1.1",
    "echarts": "^5.4.1",
    "flatpickr": "^4.6.13",
    "pinia": "^2.0.29",
    "swiper": "^9.1.0",
    "ua-parser-js": "^1.0.35",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.27.3",
    "@vitest/ui": "^0.27.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.8",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.4.1",
    "eslint": "^8.33.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.3",
    "svgo": "^3.0.2",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vitest": "^0.25.8"
  }
}
